@model FootballLeague.Data.Entities.Function

@{
    ViewData["Title"] = "Create";
}

<h2>Create Function</h2>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="NamePosition" class="control-label"></label>
                <input asp-for="NamePosition" class="form-control" />
                <span asp-validation-for="NamePosition" class="text-danger"></span>
            </div>
            <div class="form-group">
                <button type="submit" class="btn btn-primary"><i class="fas fa-save"></i> Create</button>
                <a asp-action="Index" class="btn btn-success"><i class="fas fa-chevron-left"></i> Back to List</a>
        </form>
    </div>
</div>



